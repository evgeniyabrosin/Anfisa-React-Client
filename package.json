  "scripts": {
    "start": "yarn & craco start",
    "lint": "eslint -c .eslintrc.js --ext js,ts,tsx src cypress",
    "lint:fix": "yarn lint --fix",
    "stylelint": "stylelint \"**/*.css\" --config stylelint.config.js",
    "stylelint:fix": "stylelint \"**/*.css\" --config stylelint.config.js --fix",
    "lint-stage": "lint-staged --relative",
    "commitlint": "commitlint --edit",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "simple-git-hooks": {
    "pre-commit": "yarn lint-stage && yarn lint:fix",
    "pre-push": "yarn lint && yarn stylelint",
    "commit-msg": "yarn commitlint"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
